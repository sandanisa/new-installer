mrh="\033[31m"
kun="\033[33m"
kunt="\033[33;1m"
ijo="\033[32m"
wet="\033[97;1m"
nat="\033[0m"
bir="\033[90;36m"
ijo="\033[32m"
nat="\033[0m"
read -p "List: " lis;
while read list; do
c=$(curl -s "${list}" -L | grep -oP '<title>(.*?)</title>' | cut -d ">" -f2 | cut -d "<" -f1)
if [[ $c ]]; then
echo -e " http://${list}/wp-login.php" >> good.txt
echo -e " ${ijo}[${nat}${c}${ijo}]${nat} ${ijo}${list}${nat}"
else
echo ${list} >> error.txt
fi
done < lis

